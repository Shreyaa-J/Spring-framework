<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="stud" class="com.entity.Student">

		<property name="stud_id" value="121"></property>
		<property name="stud_name" value="Shreya">
		</property>
		<property name="branch" value="CSE"></property>
		<property name="clg" ref="clg"></property>

	</bean>
	<bean id="clg" class="com.entity.College">
		<property name="c_id" value="1"></property>
		<property name="c_name" value="TKA"></property>
	</bean>


</beans>